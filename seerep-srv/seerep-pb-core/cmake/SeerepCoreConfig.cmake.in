@PACKAGE_INIT@

list(APPEND CMAKE_MODULE_PATH "${CURRENT_LIST_DIR}/Modules")

set(SeerepPbCore_INCLUDE_DIRS @PACKAGE_CMAKE_INSTALL_INCLUDEDIR@)

find_package(PkgConfig REQUIRED)
find_library(SeerepPbCore_LIBRARY NAMES seereppbcore)
set(SeerepPbCore_LIBRARIES ${SeerepPbCore_LIBRARY})

find_package(Protobuf REQUIRED)
list(APPEND SeerepPbCore_INCLUDE_DIRS ${Protobuf_INCLUDE_DIR})
list(APPEND SeerepPbCore_LIBRARIES ${Protobuf_LIBRARY})

find_package(HDF5 REQUIRED)
list(APPEND SeerepPbCore_INCLUDE_DIRS ${HDF5_INCLUDE_DIRS})
list(APPEND SeerepPbCore_LIBRARIES ${HDF5_LIBRARIES})

find_package(HighFive REQUIRED)
list(APPEND SeerepPbCore_INCLUDE_DIRS ${HighFive_INCLUDE_DIRS})
list(APPEND SeerepPbCore_LIBRARIES ${HighFive_LIBRARIES})

find_package(SeerepMsgs REQUIRED)
list(APPEND SeerepPbCore_INCLUDE_DIRS ${SeerepMsgs_INCLUDE_DIRS})
list(APPEND SeerepPbCore_LIBRARIES ${SeerepMsgs_LIBRARIES})

find_package(SeerepProtobufIO REQUIRED)
list(APPEND SeerepPbCore_INCLUDE_DIRS ${SeerepProtobufIO_INCLUDE_DIRS})
list(APPEND SeerepPbCore_LIBRARIES ${SeerepProtobufIO_LIBRARIES})

find_package(seerep_ros_conversions REQUIRED)
list(APPEND SeerepPbCore_INCLUDE_DIRS ${seerep_ros_conversions_INCLUDE_DIRS})
list(APPEND SeerepPbCore_LIBRARIES ${seerep_ros_conversions_LIBRARIES})
