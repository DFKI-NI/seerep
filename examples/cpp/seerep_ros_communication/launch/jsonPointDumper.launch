<?xml version="1.0" encoding="utf-8" ?>
<launch>
    <arg name="hdf5FolderPath" default="/seerep/seerep-data/cognitiveWeedingUos"/>
    <arg name="jsonFilePath" default="/seerep/seerep-data/cognitiveWeedingUos/classification_2023_05_24_subset.json"/>
    <arg name="detectionCategory" default="theCategory"/>

    <node name="seerep_ros_communication_jsonPointDumper" pkg="seerep_ros_communication" type="seerep_ros_communication_jsonPointDumper" output="screen">
        <rosparam param="hdf5FolderPath" subst_value="True">$(arg hdf5FolderPath)</rosparam>
        <rosparam param="jsonFilePath" subst_value="True">$(arg jsonFilePath)</rosparam>
        <rosparam param="detectionCategory" subst_value="True">$(arg detectionCategory)</rosparam>
    </node>
</launch>
