@PACKAGE_INIT@

list(APPEND CMAKE_MODULE_PATH "${CURRENT_LIST_DIR}/Modules")

set(SeerepHDF5_INCLUDE_DIRS @PACKAGE_CMAKE_INSTALL_INCLUDEDIR@)

find_package(PkgConfig REQUIRED)
find_library(SeerepHDF5_LIBRARY NAMES seerephdf5)
set(SeerepHDF5_LIBRARIES ${SeerepHDF5_LIBRARY})

find_package(Protobuf REQUIRED)
list(APPEND SeerepHDF5_INCLUDE_DIRS ${Protobuf_INCLUDE_DIR})
list(APPEND SeerepHDF5_LIBRARIES ${Protobuf_LIBRARY})

find_package(HDF5 REQUIRED)
list(APPEND SeerepHDF5_INCLUDE_DIRS ${HDF5_INCLUDE_DIRS})
list(APPEND SeerepHDF5_LIBRARIES ${HDF5_LIBRARIES})

find_package(HighFive REQUIRED)
list(APPEND SeerepHDF5_INCLUDE_DIRS ${HighFive_INCLUDE_DIRS})
list(APPEND SeerepHDF5_LIBRARIES ${HighFive_LIBRARIES})

find_package(SeerepMsgs REQUIRED)
list(APPEND SeerepHDF5_INCLUDE_DIRS ${SeerepMsgs_INCLUDE_DIRS})
list(APPEND SeerepHDF5_LIBRARIES ${SeerepMsgs_LIBRARIES})
