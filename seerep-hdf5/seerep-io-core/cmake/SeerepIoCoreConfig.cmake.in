@PACKAGE_INIT@

list(APPEND CMAKE_MODULE_PATH "${CURRENT_LIST_DIR}/Modules")

set(SeerepIoCore_INCLUDE_DIRS @PACKAGE_CMAKE_INSTALL_INCLUDEDIR@)

find_package(PkgConfig REQUIRED)
find_library(SeerepIoCore_LIBRARY NAMES seerepiocore)
set(SeerepIoCore_LIBRARIES ${SeerepIoCore_LIBRARY})

find_package(HDF5 REQUIRED)
list(APPEND SeerepIoCore_INCLUDE_DIRS ${HDF5_INCLUDE_DIRS})
list(APPEND SeerepIoCore_LIBRARIES ${HDF5_LIBRARIES})

find_package(HighFive REQUIRED)
list(APPEND SeerepIoCore_INCLUDE_DIRS ${HighFive_INCLUDE_DIRS})
list(APPEND SeerepIoCore_LIBRARIES ${HighFive_LIBRARIES})

find_package(SeerepMsgs REQUIRED)
list(APPEND SeerepIoCore_INCLUDE_DIRS ${SeerepMsgs_INCLUDE_DIRS})
list(APPEND SeerepIoCore_LIBRARIES ${SeerepMsgs_LIBRARIES})

find_package(catkin REQUIRED COMPONENTS geometry_msgs)
list(APPEND SeerepIoCore_INCLUDE_DIRS ${catkin_INCLUDE_DIRS})
list(APPEND SeerepIoCore_LIBRARIES ${catkin_LIBRARIES})
